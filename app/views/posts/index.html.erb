<% if current_user %>
  <%= link_to "Post Something", new_user_post_path(current_user.id) %>
<% end %>
<br>
<br>
<%= render partial: "list", locals: { posts: @posts } %>
<% if current_page > 0 %>
  <%= link_to "Previous Page", posts_path(page: current_page - 1) %>
<% end %>
<% if total_pages > (current_page + 1) %>
  <%= link_to "Next Page", posts_path(page: current_page + 1) %>
<% end %>
